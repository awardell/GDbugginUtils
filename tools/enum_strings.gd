static func get_property_usage_flags(flags:int) -> PackedStringArray:
	var result := PackedStringArray()
	if flags == 0 || flags == 1: #PROPERTY_USAGE_NONE
		result.append(PropertyUsageFlags[0])
		return result

	var i := 2
	while i <= PROPERTY_USAGE_SECRET:
		if flags & i:
			result.append(PropertyUsageFlags[i])
		i *= 2

	return result
